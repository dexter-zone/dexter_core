                "type": "integer",
                "format": "uint64",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "resume_create_reward_schedules"
        ],
        "properties": {
          "resume_create_reward_schedules": {
            "type": "object",
            "required": [
              "reward_schedules_creation_request_id"
            ],
            "properties": {
              "reward_schedules_creation_request_id": {
                "type": "integer",
