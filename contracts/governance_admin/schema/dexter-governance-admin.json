              "description",
              "pool_creation_request",
              "title"
            ],
            "properties": {
              "description": {
                "type": "string"
              },
              "pool_creation_request": {
                "$ref": "#/definitions/PoolCreationRequest"
              },
              "title": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "post_governance_proposal_creation_callback"
        ],
        "properties": {
          "post_governance_proposal_creation_callback": {
            "type": "object",
            "required": [
              "pool_creation_request_id"
            ],
            "properties": {
              "pool_creation_request_id": {
                "type": "integer",
                "format": "uint64",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "resume_create_pool"
        ],
        "properties": {
          "resume_create_pool": {
            "type": "object",
            "required": [
              "pool_creation_request_id"
            ],
            "properties": {
              "pool_creation_request_id": {
                "type": "integer",
                "format": "uint64",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "resume_join_pool"
        ],
        "properties": {
          "resume_join_pool": {
            "type": "object",
            "required": [
              "pool_creation_request_id"
            ],
            "properties": {
              "pool_creation_request_id": {
                "type": "integer",
                "format": "uint64",
                "minimum": 0.0
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "create_reward_schedules_proposal"
        ],
        "properties": {
          "create_reward_schedules_proposal": {
            "type": "object",
            "required": [
              "description",
              "reward_schedules",
              "title"
            ],
            "properties": {
              "description": {
                "type": "string"
              },
              "reward_schedules": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/RewardSchedule"
                }
              },
              "title": {
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      {
        "type": "object",
        "required": [
          "post_reward_schedules_proposal_creation_callback"
        ],
        "properties": {
          "post_reward_schedules_proposal_creation_callback": {
            "type": "object",
            "required": [
